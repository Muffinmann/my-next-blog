{
  "extends": "next/core-web-vitals",
  "rules": {
    "block-spacing": "error",
    "no-unused-vars": "warn",
    "space-before-blocks": "error",
    "keyword-spacing": "error",
    "space-infix-ops": "error",
    "space-unary-ops": "error",
    "semi-style": [
      "error",
      "last"
    ],
    "semi": [
      "error",
      "always"
    ],
    "rest-spread-spacing": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-trailing-spaces": "error",
    "key-spacing": "error",
    "arrow-spacing": "error",
    "comma-spacing": "error",
    "react/jsx-indent": [
      "error",
      2
    ],
    "indent": [
      "error",
      2
    ]
  }
}